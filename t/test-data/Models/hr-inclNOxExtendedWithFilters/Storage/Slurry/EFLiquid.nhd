*** output ***

+ef_cattle_liquid
  print = 20
  ++units  
    en =  kg N/m2/year
    de =  kg N/m2/Jahr
    fr =  kg N/m2/an
  ++description
    Emission factor of a specific liquid storage for cattle.
  ++formula
#    my $key = "ef_cattle_" . In(cover_type);
#    return Tech($key);
     if (not defined In(cover_type)) {
         return 0;
     }
     return $TE->{'ef_cattle_'.In(cover_type)};
    
+ef_pigs_liquid
  print = 20
  ++units  
    en =  kg N/m2/year
    de =  kg N/m2/Jahr
    fr =  kg N/m2/an
  ++description
    Emission factor of a specific liquid storage for pig.
  ++formula
#    my $key = "ef_pig_" . In(cover_type);
#    return Tech($key);
     if (not defined In(cover_type)) {
         return 0;
     }
     return $TE->{'ef_pig_'.In(cover_type)};

    
+ef_liquid_pigs
  print = 20
  ++units  
    en =  kg N/m2/year
    de =  kg N/m2/Jahr
    fr =  kg N/m2/an
  ++description
    Scaled emission factor of a specific liquid storage for pig slurry.
  ++formula
    if ( lc In(contains_pig_manure) eq 'no' ) {
      return 0;
    } elsif ( lc In(contains_pig_manure) eq 'yes' ) {
      return Val(n_into_storage_liquid_pigsratio_scalar, ::Livestock) * Out(ef_pigs_liquid);
    } else {
      writeLog({
        en => 'Invalid values for "Contains pigs slurry" (' . In(contains_pig_manure) . ')' . "\n",
        de => 'Ungültige Eingabe für "Enthält Schweinegülle" (' . In(contains_pig_manure) . ')' . "\n",
        fr => 'Entrée non valable pour "Contient du lisier de porc" (' . In(contains_pig_manure) . ')' . "\n"
      });
    }

    
+ef_liquid_cattle
  print = 20
  ++units  
    en =  kg N/m2/year
    de =  kg N/m2/Jahr
    fr =  kg N/m2/an
  ++description
    Scaled emission factor of a specific liquid storage for cattle slurry.
  ++formula
    if ( lc In(contains_cattle_manure) eq 'no' ) {
      return 0;
    } elsif ( lc In(contains_cattle_manure) eq 'yes' ) {
      return (1 - Val(n_into_storage_liquid_pigsratio_scalar, ::Livestock)) * Out(ef_cattle_liquid);
    } else {
      writeLog({
        en => 'Invalid values for "Contains cattle slurry" (' . In(contains_cattle_manure) . ')' . "\n",
        de => 'Ungültige Eingabe für "Enthält Rindergülle" (' . In(contains_cattle_manure) . ')' . "\n",
        fr => 'Entrée non valable pour "Contient du lisier de bovin" (' . In(contains_cattle_manure) . ')' . "\n"
      });
    }

 
+c_free_factor_storage_slurry
  print = 10
  ++units 
    en = -
  ++description
    Free reduction of the Emission rate for the Yard.
  ++formula
  if(In(free_correction_factor)//0 != 0) {
      writeLog({en=>"You have entered an additional emission mitigation measure for a storage of slurry of "
                    . In(free_correction_factor) 
                    . "\%!\n",
                de=>"Sie haben eine zusätzliche emissionsmindernde Massnahme für ein Güllelager von "
                    . In(free_correction_factor) 
                    . "\% eingegeben!\n", 
                fr=>"Vous avez introduit une mesure supplémentaire limitant les émissions "
                    . "du stock de lisier de " . In(free_correction_factor) . "\%.\n" });
            return 1 - In(free_correction_factor)/100;
     
  } else {
    return 1;
  } 
