*** external ***

*** output ***
    
+er_housing
  print = 15
  ++units  
    en = -
  ++description
    Emission rate for specific housing type.
  ++formula
    Tech(er);

+share_liquid
  print = 15
  ++units  
    en = -
  ++description    
    Part of Ntot flowing into liquid storage for selected housing type.
  ++formula
    Tech(share_liquid);

+share_solid
  print = 15
  ++units  
    en = -
  ++description
    Share of solid manure of Ntot for selected housing type.
  ++formula
    1-Tech(share_liquid);

+k_area
  print = 15
  ++units  
    en = -
  ++description
    Correction factor for area per animal.
  ++formula
    Tech(k_area);
