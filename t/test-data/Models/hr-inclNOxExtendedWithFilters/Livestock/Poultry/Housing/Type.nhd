+er_housing
  print = 10
  ++units  
    en = -
  ++description
    Emission rate for the poultry housing type.
  ++formula
    if (Val(animalcategory, ..::Excretion) eq "layers"){
        if (In(housing_type) eq "manure_belt_without_manure_belt_drying_system"){
   	        return Tech(er_housing_layers_growers_manure_belt_without_manure_belt_drying_system);
        } 
        elsif (In(housing_type) eq "manure_belt_with_manure_belt_drying_system"){
            return Tech(er_housing_layers_growers_manure_belt_with_manure_belt_drying_system);
      	} 
        elsif (In(housing_type) eq "deep_pit"){
            return Tech(er_housing_layers_growers_deep_pit);
      	} 
        elsif(In(housing_type) eq "deep_litter"){
     	    return Tech(er_housing_layers_growers_deep_litter);
        }
	}
    elsif (Val(animalcategory, ..::Excretion) eq "growers"){
    	if (In(housing_type) eq "manure_belt_without_manure_belt_drying_system"){
       	  	return Tech(er_housing_layers_growers_manure_belt_without_manure_belt_drying_system);
       	}
	elsif (In(housing_type) eq "manure_belt_with_manure_belt_drying_system"){
            return Tech(er_housing_layers_growers_manure_belt_with_manure_belt_drying_system);
      	} 
        elsif (In(housing_type) eq "deep_pit"){
      	    return Tech(er_housing_layers_growers_deep_pit);
        } 
        elsif(In(housing_type) eq "deep_litter"){
     	    return Tech(er_housing_layers_growers_deep_litter);
        }
    }
    elsif (Val(animalcategory, ..::Excretion) eq "broilers"){
        if (In(housing_type) eq "manure_belt_without_manure_belt_drying_system"){
	        writeLog({en => "The category manure belt without manure belt drying system is not valid, please select the housing system deep litter for broilers.",
