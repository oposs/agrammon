+share_outdoor
  print=15
  ++units
     en = -
  ++description
    Factor for considering outdoor part.
  ++formula 
    if ( (Out(housing_type) eq 'Slurry_Label') or (Out(housing_type) eq 'Slurry_Label_Open') ){
       return 0.5;
    }
    else {
       return( 1 );
    }


+c_exercise_place
  print = 15
  ++units  
    en = -
  ++description
    Reduction exercise place.
  ++formula
    if (defined In(exercise_place) and In(exercise_place) eq 'exercise_place' ){ 
        if ((Out(housing_type) eq 'Slurry_Conventional') or (Out(housing_type) eq 'Deep_Litter') or (Out(housing_type) eq 'Outdoor')) {
	    writeLog({en=>"An outside exercise yard is applicable for the label system only!",
                 de=>"Auslauf ist nur für Labelställe anwendbar!",
                 fr=>"Parcour exterieur est éligible pour les stabulations label seulement!"});
	    return 0;
	}
	else {
	     return( Tech(red_exercise_place));
	}
    }
    else {
	    return 0;
       } ;
