#?if Single_extendedOutput

+animals
  print = RoughageConsuming
  ++units
    en = -
  ++description
    Number of dairy cows in barn.
  ++formula
    Val(animals, RoughageConsuming::Excretion);

+animalcategory
  print = RoughageConsuming
  ++units
    en = -
  ++description
    Animal category
  ++formula
    Val(animalcategory, RoughageConsuming::Excretion);


