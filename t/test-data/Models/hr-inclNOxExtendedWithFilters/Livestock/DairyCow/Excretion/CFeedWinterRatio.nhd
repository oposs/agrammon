+share_hay_only_winter
  print = 22
  ++units 
    en = %
  ++description
    Share
  ++formula
    if ( Out(share_grass_silage_winter) >= Out(share_maize_silage_winter) && Out(share_grass_silage_winter) >= Out(share_maize_pellets_winter)) {
       return( 1 - Out(share_grass_silage_winter) );
    } elsif ( Out(share_maize_silage_winter) >= Out(share_grass_silage_winter) && Out(share_maize_silage_winter) >= Out(share_maize_pellets_winter) ) {
       return( 1 - Out(share_maize_silage_winter) );
    }else{
      return( 1 - Out(share_maize_pellets_winter));
    }

 

+c_winter_ration
  print = 15
  ++units 
    en = %
  ++description
    Winter feed ration correction factor for annual N excretion.
  ++formula
    Tech(c_grass_silage_winter) * Out(share_grass_silage_winter) + 
    Tech(c_maize_silage_winter) * Out(share_maize_silage_winter) +
    Tech(c_maize_pellets_winter) * Out(share_maize_pellets_winter) +
    Tech(c_potatoes_winter) * Out(share_potatoes_winter) + 
    Tech(c_beets_winter) * Out(share_beets_winter) +
    Tech(c_default_hay) * Out(share_hay_only_winter) ;
