*** output ***

+c_UNECE
  print = 15
  ++units 
    en = -
  ++description
    Reduction factor for the emission due to the use of a grooved floor in housing systems.
  ++formula
      # TODO: is next line correct? Was missing compared to OtherCattle
      return 0 unless defined In(UNECE_category_1_mitigation_options_for_housing_systems_for_dairy_cows);
      if (In(UNECE_category_1_mitigation_options_for_housing_systems_for_dairy_cows) eq 'raised_feeding_stands'){
          return( Tech(red_raised_feeding_stands) );
      }
      elsif (In(UNECE_category_1_mitigation_options_for_housing_systems_for_dairy_cows) eq 'floor_with_cross_slope_and_collection_gutter'){
          return( Tech(red_floor_with_cross_slope_and_collection_gutter) );
      }
      elsif (In(UNECE_category_1_mitigation_options_for_housing_systems_for_dairy_cows) eq 'floor_with_cross_slope_and_collection_gutter_and_raised_feeding_stands'){
          return( Tech(red_floor_with_cross_slope_and_collection_gutter_and_raised_feeding_stands) );
      }
      elsif (In(UNECE_category_1_mitigation_options_for_housing_systems_for_dairy_cows) eq 'toothed_scrapper_running_over_a_grooved_floor'){
          return( Tech(red_toothed_scrapper_running_over_a_grooved_floor) );
      }
      else {
          return 0;
      }


