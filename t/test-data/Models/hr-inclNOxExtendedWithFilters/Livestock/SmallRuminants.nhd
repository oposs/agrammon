#?if Single_extendedOutput

+animals
  print = SmallRuminants
  ++units
    en = -
  ++description
    Number of dairy cows in barn.
  ++formula
    Val(animals, SmallRuminants::Excretion);

+animalcategory
  print = SmallRuminants
  ++units
    en = -
  ++description
    Animal category
  ++formula
    Val(animalcategory, SmallRuminants::Excretion);


