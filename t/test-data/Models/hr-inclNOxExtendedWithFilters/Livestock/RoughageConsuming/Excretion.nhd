*** external ***

*** output ***

+animals
  print = RoughageConsuming
  ++units
    en = -
  ++description
    Number of small ruminants for the selected type in barn.
  ++formula
    In(animals);

+animalcategory
  print = RoughageConsuming
  ++units
    en = -
  ++description
    Animal category
  ++formula
    In(animalcategory);

+n_excretion_animal
  print = RoughageConsuming
  ++units
    en = kg N/year
    de = kg N/Jahr
    fr = kg N/an
  ++description
    Annual standard N excretion for specified other animal category according to
    Flisch et al. (2009).
?if Kantonal_LU
  ++formula
    return $TE->{'standard_N_excretion_' . Out(animalcategory)};
?else
  ++formula
    my $cat = Out(animalcategory);
    my $key = 'standard_N_excretion_' . $cat;
    my $exc = $TE->{$key};
    $exc = In(inp_n_excretion) unless ( (not defined In(inp_n_excretion)) or lc In(inp_n_excretion) eq 'standard' );
    if ( ($exc < 0.7 * $TE->{$key}) or ($exc > 1.3 * $TE->{$key})) {
        writeLog({
            en => "Die eingegebene N-Ausscheidung für $cat weicht mehr als 30% vom Standard ab.",
            de => "Die eingegebene N-Ausscheidung für $cat weicht mehr als 30% vom Standard ab.",
            fr => "Die eingegebene N-Ausscheidung für $cat weicht mehr als 30% vom Standard ab."
        });
    }
    return $exc;
?endif

+n_excretion
  print = RoughageConsuming
  ++units
    en = kg N/year
    de = kg N/Jahr
    fr = kg N/an
  ++description
    Annual N excreted by a specified number of small ruminants.
  ++formula
    Out(n_excretion_animal) * Out(animals);

+tan_excretion
  print = RoughageConsuming
  ++units
    en = kg N/year
    de = kg N/Jahr
    fr = kg N/an
  ++description
    Annual soluble N excreted by a specified number of small ruminants.
?if !Kantonal_LU
  ++formula
    if ( (not defined In(tan_fraction)) or lc In(tan_fraction) eq 'standard' ) {
      $TE->{"share_Nsol_" . Out(animalcategory)} * Out(n_excretion);
    } else {
      my $tan = In(tan_fraction);
      $tan = In(tan_fraction) / 100;
      if ( abs($tan - $TE->{"share_Nsol_" . Out(animalcategory)}) > 0.2) {
        writeLog({
            en => "TODO: Der eingegebene TAN Gehalt weicht um mehr als 20% vom Standardwert ab!",
            de => "Der eingegebene TAN Gehalt weicht um mehr als 20% vom Standardwert ab!",
            fr => "TODO: Der eingegebene TAN Gehalt weicht um mehr als 20% vom Standardwert ab!"
        });
      }
      return $tan * Out(n_excretion);
    }
?else
  ++formula
    $TE->{"share_Nsol_" . Out(animalcategory)} * Out(n_excretion);
?endif
